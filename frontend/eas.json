{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "cache": {
        "disabled": true
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease --no-daemon"
      },
      "env": {
        "EAS_BUILD_RUNNER": "eas-build"
      }
    }
  },
  "submit": {
    "production": {}
  }
}